# user table
userCreateTable=CREATE TABLE IF NOT EXISTS `user` (`id` bigint NOT NULL AUTO_INCREMENT, `name` varchar(255) NOT NULL, `last_name` varchar(255) NOT NULL, `age` tinyint NOT NULL, PRIMARY KEY (`id`)) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3;
userTruncateTable=TRUNCATE TABLE IF EXISTS user
userDropTable=DROP TABLE IF EXISTS user
# user data
userCreate=INSERT INTO user (name, last_name, age) VALUES (?, ?, ?)
userFindAll=SELECT id, name, last_name, age FROM user
userRemove=DELETE FROM user WHERE id = ?
userRemoveAll=DELETE FROM user